{
    "name": "lmly9193/fastphp",
    "type": "project",
    "description": "FastPHP is a simple and lightweight PHP project template.",
    "keywords": ["FastRoute", "Symfony"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "nikic/fast-route": "^1.3",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/http-foundation": "^7.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
